@page

@model IEnumerable<her_care.Models.Test>

@{
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
        <meta name="viewport" content="width-device-width"/>
        <title>SearchResults</title>
        <link rel="site" href="~/css/site.css" /> 
    </head>




<div>
  
    <table width="60%" align="center">
    <tr align="center">
        <td>
            <h1>Client and Volunteer Search</h1>
            <h1>Search Results</h1>
        </td>
    </tr>
    <tr align="center">
        <td>

            <table border="1" width="100%">
                <tr>
                    <td><strong>ID</strong></td>
                    <td><strong>First Name</strong></td>
                    <td><strong>Last Name</strong></td>
                </tr>
               
                    
                
                @foreach (var result in ViewBag.MyModel)
                { 
                   
                    <tr>
                        <td>@result.Id</td>
                        <td>@result.FName</td>
                        <td>@result.LName</td>                        
                    </tr>
                
                }
                
        </td>
    </tr>
</table>
</table><a asp-page="~/Pages/Shared/Client">Click here for client page</a>
</div>


